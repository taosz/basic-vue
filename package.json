{
	"name": "basic-vue",
	"version": "0.1.1",
	"description": "basic env for vue",
	"main": "index.js",
	"scripts": {
		"start": "npm run dev",
		"dev": "npm run prompt && npm run all",
		"serve": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  ./config/webpack.config.dev.js --hot",
		"dist": "npm run remove:dist && npm run lint && node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config ./config/webpack.config.dist.js",
		"build": "cross-env UGLIFY_JS=true npm run dist -p && npm run copy",
		"_build": "npm run lint:fix && npm run dist && npm publish --access public",
		"build:dev": "cross-env UGLIFY_JS=false npm run dist && npm run copy",
		"remove:dist": "rimraf -rf ./dist",
		"clear": "rimraf -rf ./node_modules/.cache",
		"copy": "echo 'end'",
		"lint": "esw config ./src/pages --ext .vue,.js",
		"lint:fix": "npm run lint --fix",
		"lint:watch": "npm run lint --fix --watch",
		"lint:style": "stylelint --fix --syntax scss src",
		"mock": "cd ./src/mock && npm run start",
		"prompt": "node ./config/prompt.config.js",
		"all": "cross-env BB=development npm-run-all --parallel --race --continue-on-error lint:watch serve",
		"lint-staged": "lint-staged",
		"pre-commit:install": "cp .hooks/* .git/hooks/",
		"create": "basic-toolkit add --config ./config/toolkit.config.js",
		"test": "jest --config ./tests/config/jest.config.js"
	},
	"author": "taosz",
	"license": "MIT",
	"engines": {
		"node": ">=6"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/plugin-proposal-decorators": "^7.3.0",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.2.0",
		"@babel/plugin-proposal-function-bind": "^7.2.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-block-scoping": "^7.2.0",
		"@babel/preset-env": "^7.3.1",
		"@wya/vc-loader": "^0.2.3",
		"assets-webpack-plugin": "^3.5.1",
		"autoprefixer": "^8.3.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^24.0.0",
		"babel-loader": "^8.0.4",
		"babel-plugin-import": "^1.11.0",
		"copy-webpack-plugin": "^4.5.1",
		"cross-env": "^5.1.4",
		"css-loader": "^2.1.0",
		"cssnano": "^4.1.10",
		"enzyme": "^3.5.0",
		"enzyme-to-json": "^3.3.4",
		"eslint": "^5.13.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-babel": "^5.0.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-markdown": "^1.0.0-beta.6",
		"eslint-plugin-vue": "^4.7.1",
		"eslint-watch": "^4.0.2",
		"file-loader": "^1.1.11",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"fs-extra": "^5.0.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"inquirer": "^5.2.0",
		"jest": "^24.0.0",
		"lint-staged": "^7.0.4",
		"mini-css-extract-plugin": "^0.5.0",
		"node-sass": "^4.8.3",
		"npm-run-all": "^4.1.2",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"pre-commit": "^1.2.2",
		"precss": "^4.0.0",
		"rimraf": "^2.6.1",
		"sass-loader": "^7.0.1",
		"sass-resources-loader": "^2.0.0",
		"shelljs": "^0.8.3",
		"stylelint": "^9.2.0",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-webpack-plugin": "^0.10.5",
		"url-loader": "^1.0.1",
		"vue-loader": "^15.6.2",
		"vue-style-loader": "^4.1.1",
		"vue-template-compiler": "^2.6.10",
		"webpack": "~4.27.0",
		"webpack-bundle-analyzer": "^3.0.3",
		"webpack-cli": "^3.2.1",
		"webpack-dev-server": "^3.1.14",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
		"@vue/babel-preset-jsx": "^1.1.0",
		"@wya/http": "^1.1.7",
		"@wya/ps": "^1.0.2",
		"@wya/qrcode": "^1.0.2",
		"@wya/sass": "^1.1.10",
		"@wya/socket": "^1.0.0",
		"@wya/utils": "^1.3.3",
		"@wya/vc": "^1.5.2",
		"@wya/vm": "^1.1.11",
		"axios": "^0.19.0",
		"element-ui": "^2.12.0",
		"eruda": "^1.5.8",
		"js-md5": "^0.7.3",
		"lodash": "^4.17.15",
		"mockjs": "^1.0.1-beta3",
		"moment": "^2.24.0",
		"vue": "^2.6.7",
		"vue-router": "^3.0.2",
		"vuex": "^3.0.1",
		"vuex-router-sync": "^5.0.0"
	},
	"pre-commit": [
		"lint-staged"
	],
	"lint-staged": {
		"{src,bin}/pages/*": [
			"esw --fix",
			"git add"
		],
		"*.md": [
			"esw --ext '.md' --fix",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/taosz/basic-vue.git"
	}
}
