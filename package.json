    "@wya/sass": "^1.1.5",
    "@wya/socket": "^1.0.0",
    "@wya/utils": "^1.3.1",
    "@wya/vc": "^1.4.29",
    "@wya/vm": "^1.1.1",
    "ali-oss": "^6.1.0",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "connect": "^3.6.6",
