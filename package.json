    "ws": "^6.0.0"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "{src,bin}/**/*": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taosz/basic-vue.git"
  }
}
