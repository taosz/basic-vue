    "fs-extra": "^5.0.0",
    "inquirer": "^5.2.0",
    "js-md5": "^0.7.3",
    "moment": "^2.24.0",
    "ora": "^2.0.0",
    "screenshot-desktop": "^1.5.2",
    "serve-index": "^1.9.1",
    "serve-static": "^1.13.2",
    "shelljs": "^0.8.2",
    "upath": "^1.0.5",
    "vue": "^2.6.7",
